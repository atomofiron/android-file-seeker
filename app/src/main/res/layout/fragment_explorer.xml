<?xml version="1.0" encoding="utf-8"?>
<app.atomofiron.searchboxapp.custom.view.layout.RootFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?colorBackground"
    >
    <androidx.viewpager2.widget.ViewPager2
        android:id="@+id/pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
    <app.atomofiron.searchboxapp.custom.view.InsetsBackgroundView
        android:id="@+id/system_ui_background"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:navigationBar="none"
        />
    <TextView
        android:id="@+id/disclaimer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/padding_mini"
        android:paddingHorizontal="@dimen/padding_half"
        android:gravity="center"
        android:textStyle="bold"
        android:textSize="16sp"
        tools:text="Please do something"
        />
    <app.atomofiron.searchboxapp.custom.view.SnackbarContainerLayout
        android:id="@+id/snackbar_container"
        android:layout_width="0dp"
        android:layout_height="0dp"
        />
    <app.atomofiron.searchboxapp.custom.view.dock.DockBarView
        android:id="@+id/dock_bar"
        android:layout_width="@dimen/dock_width"
        android:layout_height="@dimen/dock_height"
        />
</app.atomofiron.searchboxapp.custom.view.layout.RootFrameLayout>
